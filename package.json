{
  "name": "@masa-finance/masa-sdk",
  "version": "0.5.2",
  "description": "Brand new masa SDK",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "unpkg": "./dist/browser/masa-sdk.cjs2.min.js",
  "module": "./dist/browser/masa-sdk.amd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn build:watch",
    "build": "yarn build:tsc && yarn build:dist",
    "build:tsc": "tsc",
    "build:metadata": "./scripts/get-metadata.js > src/metadata.json",
    "build:dist": "cross-env NODE_ENV=production webpack",
    "build:watch": "tsc -w",
    "prettier": "prettier ./src --write",
    "lint": "eslint ./src",
    "doc": "typedoc",
    "prepublishOnly": "yarn build",
    "publish-lib": "npm publish",
    "publish-beta": "npm publish --tag beta",
    "publish-dryrun": "npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masa-finance/masa-sdk.git"
  },
  "keywords": [
    "De-fi"
  ],
  "author": "Masa Finance",
  "license": "MIT",
  "dependencies": {
    "@masa-finance/masa-contracts-identity": "^0.4.2",
    "arweave": "^1.11.6",
    "axios": "1.1.0",
    "ethers": "^5.7.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.6",
    "typedoc": "^0.23.18",
    "typedoc-plugin-markdown": "^3.13.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
