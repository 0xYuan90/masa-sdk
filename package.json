{
  "name": "@masa-finance/masa-sdk",
  "version": "0.3.2",
  "description": "Brand new masa SDK",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "module": "dist/lib/masa-sdk.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc --watch",
    "compile": "tsc",
    "build": "yarn compile && yarn bundle && yarn bundle:minify",
    "bundle": "esbuild ./src/index.ts --bundle --outfile=./dist/lib/masa-sdk.esm.js",
    "bundle:minify": "yarn bundle --minify --sourcemap --outfile=./dist/lib/masa-sdk.esm.min.js",
    "prettier": "prettier ./src --write",
    "lint": "eslint ./src",
    "doc": "typedoc",
    "prepublishOnly": "yarn build",
    "publish-lib": "npm publish",
    "publish-beta": "npm publish --tag beta",
    "publish-dryrun": "npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masa-finance/masa-sdk.git"
  },
  "keywords": [
    "De-fi"
  ],
  "author": "Masa Finance",
  "license": "MIT",
  "dependencies": {
    "@masa-finance/masa-contracts-identity": "^0.4.2",
    "arweave": "^1.11.6",
    "axios": "^1.1.3",
    "ethers": "^5.7.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "esbuild": "^0.15.13",
    "eslint": "^8.25.0",
    "prettier": "^2.7.1",
    "typedoc": "^0.23.18",
    "typedoc-plugin-markdown": "^3.13.6"
  }
}
